#include <iostream>
using namespace std;

int main(){
    int tugas;
    int uts;
    int uas;
   
        cout<<"masukan nilai tugas = ";cin>>tugas;
        if(tugas > 80){
           cout<<"nilai tidak dapat memenuhi" ;
           return 0;
        }
        cout<<"masukan nilai uts = ";cin>>uts;
        if (uts > 80){
            cout<<"nilai tidak dapat memenuhi" ;
            return 0;
        }
        cout<<"masukan nilai uas = ";cin>>uts;
        if (uas > 80){
            cout<<"nilai tidak dapat memenuhi" ;
            return 0 ;
        }
        
       
}
