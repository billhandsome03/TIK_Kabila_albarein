 #include <iostream>
 using namespace std;
 int main (){
     float cm, km, m, hm, dam, meter, mm,dm,a,b,c,d,e,f;
     cout<<"konversi";
     cout<<endl;
     cout<<"masukan angka cm:";
     cin>>a;
     km=a/1000000;
     hm=a/100000;
     dam=a/10000;
     meter=a/1000;
     dm=a/100;
     mm=a*10;
     
     cout<<endl<<endl;
     cout<<"km :"<<km<<endl;
     cout<<"hm :"<<hm<<endl;
     cout<<"dam :"<<dam<<endl;
     cout<<"m :"<<m<<endl;
     cout<<"dm :"<<dm<<endl;
     cout<<"cm :"<<a<<endl;
     cout<<"mm :"<<hm<<endl;
     
 }
