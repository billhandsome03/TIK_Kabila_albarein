#include <iostream>
using namespace std;

int main(){
    /* 
    jika nilai >=90 sangat memuaskan 
    jika nilai >=80 memuaskan
    jika nilai >=75 cukup
    nilai <75 tidak memuaskan  
    nilai tidak boleh > 100 dan <0 keterangan diluar jangkauan.
     */
    int nilai;
    cout<<"masukan nilai = ";cin>>nilai;
    
    if (nilai >= 90 ){ 
        cout<<"sangat memuaskan";
    }
    else if (nilai >= 80){
        cout<<"memuaskan";
    }
    else {
        cout<<"tidak lulus";
    }
    
    
}
